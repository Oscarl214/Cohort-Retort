{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/build",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/build/index.html"
    }
  ]
}
